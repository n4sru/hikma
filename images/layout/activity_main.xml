<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/booksbutt"
        style="@android:style/Widget.ImageButton"
        android:layout_width="115dp"
        android:layout_height="105dp"
        android:layout_marginBottom="38dp"
        android:layout_marginEnd="20dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="20dp"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toTopOf="@+id/infobutt"
        app:layout_constraintEnd_toStartOf="@+id/lampbutt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_foreground" />

    <ImageButton
        android:id="@+id/lampbutt"
        style="@android:style/Widget.ImageButton"
        android:layout_width="115dp"
        android:layout_height="105dp"
        android:layout_marginBottom="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/diplombutt"
        app:layout_constraintStart_toEndOf="@+id/booksbutt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/lampl" />

    <ImageButton
        android:id="@+id/diplombutt"
        style="@android:style/Widget.ImageButton"
        android:layout_width="115dp"
        android:layout_height="105dp"
        android:layout_marginBottom="38dp"
        android:layout_marginEnd="32dp"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="32dp"
        android:layout_marginStart="9dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lampbutt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/diploma" />

    <ImageButton
        android:id="@+id/infobutt"
        style="@android:style/Widget.ImageButton"
        android:layout_width="115dp"
        android:layout_height="105dp"
        android:layout_marginBottom="25dp"
        android:layout_marginTop="38dp"
        app:layout_constraintBottom_toTopOf="@+id/howbutt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/booksbutt"
        app:srcCompat="@mipmap/inform" />

    <ImageButton
        android:id="@+id/howbutt"
        style="@android:style/Widget.ImageButton"
        android:layout_width="115dp"
        android:layout_height="105dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/infobutt"
        app:srcCompat="@mipmap/question" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="164dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="8dp"
        android:text="MADOOYIN"
        android:textAppearance="@android:style/TextAppearance.WindowTitle"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lampbutt" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:text="INTAXAAN"
        android:textAppearance="@android:style/TextAppearance.WindowTitle"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/diplombutt" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WAA AYO ESTUDIES"
        android:textAppearance="@android:style/TextAppearance.WindowTitle"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/infobutt"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QAABKAN OGA FAIDEYSO"
        android:textAppearance="@android:style/TextAppearance.WindowTitle"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/howbutt"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="134dp"
        android:layout_height="154dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView35"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/howbutt"
        app:layout_constraintVertical_bias="0.142"
        app:srcCompat="@mipmap/pen" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="132dp"
        android:layout_height="174dp"
        android:layout_marginEnd="180dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="180dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/howbutt"
        app:layout_constraintVertical_bias="0.288"
        app:srcCompat="@mipmap/pen" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="309dp"
        android:layout_height="201dp"
        android:layout_marginEnd="12dp"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="12dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="316dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.83"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView33"
        app:layout_constraintVertical_bias="0.492"
        app:srcCompat="@mipmap/pen" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.828"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.7"
        app:srcCompat="@mipmap/pen" />


</android.support.constraint.ConstraintLayout>